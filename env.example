# Vite Environment Variables
# Copy this file to .env.local and fill in your actual values

# Clerk Authentication (Frontend)
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
VITE_CLERK_DOMAIN=your_clerk_domain_here

# Clerk Authentication (Backend - for Convex integration)
CLERK_APPLICATION_ID=your_clerk_application_id_here
CLERK_PRIVATE_KEY=your_clerk_private_key_here
CLERK_JWKS_URL=your_clerk_jwks_url_here
CLERK_ISSUER_DOMAIN=your_clerk_issuer_domain_here

# Convex Backend
VITE_CONVEX_URL=your_convex_url_here

# UploadThing (Image Upload Service)
# Get your API key from https://uploadthing.com/dashboard
VITE_UPLOADTHING_TOKEN=your_uploadthing_api_key_here